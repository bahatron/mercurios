{
    "name": "@bahatron/mercurios",
    "version": "2.4.2",
    "description": "mercurios client",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "preversion": "npm run prepublishOnly",
        "prepublishOnly": "npm run build:clean && npm run test",
        "prepare": "npm run build",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "build:clean": "rm -rf dist && tsc",
        "test": "jest --forceExit",
        "test:ts": "jest --forceExit --config=jest.unit.config.js",
        "test:ci": "jest --silent --forceExit --reporters=default --reporters=jest-stare"
    },
    "author": "simonpiscitelli@gmail.com",
    "license": "Apache-2.0",
    "dependencies": {
        "@bahatron/utils": "^2.4.0",
        "axios": "^0.21.4",
