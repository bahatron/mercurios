        "pg": "^8.7.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/jest": "^27.0.1",
        "@types/luxon": "^1.27.1",
        "@types/mocha": "^8.2.3",
        "@types/node": "^14.17.15",
        "@types/uuid": "^8.3.1",
        "jest": "^27.2.0",
        "jest-stare": "^2.3.0",
        "ts-jest": "^27.0.5",
        "ts-node": "^9.1.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    }
